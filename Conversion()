void conversion(string x) {
	vector<char> data(x.begin(), x.end());
	for (int i = 0; i < data.size(); i++) {
		cout << data.at(i);
	}
}
void conversionToString(vector<char>& x) {
	string s(x.begin(), x.end());
	cout << s;
}
int main() {
	conversion("hello");
	return 0;
}
