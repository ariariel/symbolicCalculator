vector<char> RPNVec;

void add(index1, index2){
	index1 = index1 + index2;

	//we are changing index 1- index 2 is still the same
	//deleting index two, moving everything over

	RPNVec[index1 + 1].push_back(); //assuming index two is void/zero

	for(int i = 0; i < RPNVec.size(); i++){


	}

}
void subtract(index1, index2){
	index1 = index1 - index2;
}
void multiply(index1, index2){
	index1 = index1 * index2;
}
void divide(index1, index2){
	index1 = index1 / index2;
}
string power(index1, index2){
	s = pow(index1, index2);
	return s;
}


